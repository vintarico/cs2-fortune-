// Função para gerar imagem da skin
function getSkinImage(weaponName) {
  // Remove a condição de desgaste do nome para buscar a imagem base
  const baseName = weaponName.replace(/ \((Factory New|Minimal Wear|Field-Tested|Well-Worn|Battle-Scarred)\)$/, '');
  
  // Busca índice da imagem no mapeamento por categoria
  for (const category in WEAPON_IMAGE_MAP) {
    if (WEAPON_IMAGE_MAP[category][baseName] !== undefined) {
      const imageIndex = WEAPON_IMAGE_MAP[category][baseName];
      return `/skins/${AVAILABLE_IMAGES[imageIndex]}`;
    }
  }
  
  // Fallback: usar hash do nome para distribuir imagens uniformemente
  const hash = baseName.split('').reduce((acc, char) => acc + char.charCodeAt(0), 0);
  const imageIndex = hash % AVAILABLE_IMAGES.length;
  return `/skins/${AVAILABLE_IMAGES[imageIndex]}`;
}
  
