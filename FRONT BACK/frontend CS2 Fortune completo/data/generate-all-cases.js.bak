// Função para gerar imagem da skin
function getSkinImage(weaponName) {
  // Remove a condição de desgaste do nome para buscar a imagem base
  const baseName = weaponName.replace(/ \((Factory New|Minimal Wear|Field-Tested|Well-Worn|Battle-Scarred)\)$/, '');
  
  // Busca a imagem no mapeamento
  const imagePath = SKIN_IMAGE_MAP[baseName] || SKIN_IMAGE_MAP["default"];
  
  // Retorna o caminho completo
  return`/skins/${imagePath}`;
}

// Gerar 20+ itens para uma caixa
function generateCaseItems(casePrice) {
  const items = [];
  
  // 1 EXCEEDINGLY RARE (Faca)
  const knifeWeapon = getRandomWeapon('knives');
  items.push({
    name: `${knifeWeapon} (Factory New)`,
    rarity: "Exceedingly Rare",
    chance: RARITIES['Exceedingly Rare'].chance,
    value: randomValue(
      casePrice * RARITIES['Exceedingly Rare'].minMultiplier,
      casePrice * RARITIES['Exceedingly Rare'].maxMultiplier
    ),
    image: getSkinImage(`${knifeWeapon} (Factory New)`),
    weapon: knifeWeapon.split('|')[0].trim(),
    skin: knifeWeapon.split('|')[1]?.trim() || "Vanilla",
    category: "knives"
  });

  // 2 COVERT (Vermelho)
  for (let i = 0; i < 2; i++) {
    const category = i === 0 ? 'snipers' : 'rifles';
    const weapon = getRandomWeapon(category);
    const wear = WEAR_CONDITIONS[Math.floor(Math.random() * 2)]; // FN ou MW
    items.push({
      name: `${weapon} (${wear})`,
      rarity: "Covert",
      chance: RARITIES['Covert'].chance,
      value: randomValue(
        casePrice * RARITIES['Covert'].minMultiplier,
        casePrice * RARITIES['Covert'].maxMultiplier
      ),
      image: getSkinImage(`${weapon} (${wear})`),
      weapon: weapon.split('|')[0].trim(),
      skin: weapon.split('|')[1]?.trim() || "Skin",
      category: category
    });
  }

  // 3 CLASSIFIED (Rosa)
  for (let i = 0; i < 3; i++) {
    const categories = ['rifles', 'pistols', 'smgs'];
    const weapon = getRandomWeapon(categories[i % categories.length]);
    const wear = WEAR_CONDITIONS[Math.floor(Math.random() * 3)];
    items.push({
      name: `${weapon} (${wear})`,
      rarity: "Classified",
      chance: RARITIES['Classified'].chance,
      value: randomValue(
        casePrice * RARITIES['Classified'].minMultiplier,
        casePrice * RARITIES['Classified'].maxMultiplier
      ),
      image: getSkinImage(`${weapon} (${wear})`),
      weapon: weapon.split('|')[0].trim(),
      skin: weapon.split('|')[1]?.trim() || "Skin",
      category: categories[i % categories.length]
    });
  }

  // 5 RESTRICTED (Roxo)
  for (let i = 0; i < 5; i++) {
    const categories = ['rifles', 'pistols', 'smgs', 'shotguns', 'snipers'];
    const weapon = getRandomWeapon(categories[i % categories.length]);
    const wear = WEAR_CONDITIONS[Math.floor(Math.random() * 4)];
    items.push({
      name: `${weapon} (${wear})`,
      rarity: "Restricted",
      chance: RARITIES['Restricted'].chance,
      value: randomValue(
        casePrice * RARITIES['Restricted'].minMultiplier,
        casePrice * RARITIES['Restricted'].maxMultiplier
      ),
      image: getSkinImage(`${weapon} (${wear})`),
      weapon: weapon.split('|')[0].trim(),
      skin: weapon.split('|')[1]?.trim() || "Skin",
      category: categories[i % categories.length]
    });
  }

  // 12 MIL-SPEC (Azul) - para completar 23 itens
  for (let i = 0; i < 12; i++) {
    const allCategories = ['rifles', 'pistols', 'smgs', 'shotguns', 'snipers', 'machineguns'];
    const weapon = getRandomWeapon(allCategories[i % allCategories.length]);
    const wear = WEAR_CONDITIONS[Math.floor(Math.random() * 5)];
    items.push({
      name: `${weapon} (${wear})`,
      rarity: "Mil-Spec",
      chance: RARITIES['Mil-Spec'].chance,
      value: randomValue(
        casePrice * RARITIES['Mil-Spec'].minMultiplier,
        casePrice * RARITIES['Mil-Spec'].maxMultiplier
      ),
      image: getSkinImage(`${weapon} (${wear})`),
      weapon: weapon.split('|')[0].trim(),
      skin: weapon.split('|')[1]?.trim() || "Skin",
      category: allCategories[i % allCategories.length]
    });
  }

  return items;
}

// Gerar imagem da caixa
function getCaseImage(caseId) {
  // Caixas ORIGINAIS do CS2 - Tentar imagem local primeiro, depois Steam CDN
  const steamCases = {
    "danger-zone": "/images/cases/danger-zone.png",
    "dreams-nightmares": "/images/cases/dreams-nightmares.png",
    "broken-fang": "/images/cases/broken-fang.png",
    "kilowatt": "/images/cases/kilowatt.png",
    "breakout": "/images/cases/breakout.png",
    "recoil": "/images/cases/recoil.png",
    "hydra": "/images/cases/hydra.png",
    "revolution": "/images/cases/revolution.png",
    "phoenix": "/images/cases/phoenix.png",
    "riptide": "/images/cases/riptide.png",
    "huntsman": "/images/cases/huntsman.png",
    "fracture": "/images/cases/fracture.png",
    "spectrum": "/images/cases/spectrum.png",
    "bravo": "/images/cases/bravo.png",
    "weapon-case": "/images/cases/weapon-case.png",
  };
  
  // Caixas CUSTOMIZADAS do CS2 Fortune - usar imagem genérica CS2 Fortune
  if (steamCases[caseId]) {
    return steamCases[caseId];
  }
  
  // Caixas customizadas usam imagem padrão do CS2 Fortune
  return "/images/cases/custom-case.png";
}

// Gerar todas as caixas
function generateAllCases() {
  const allCases = CASE_DEFINITIONS.map(caseDef => {
    const items = generateCaseItems(caseDef.price);
    const totalValue = items.reduce((sum, item) => sum + item.value, 0);
    
    return {
      id: caseDef.id,
      name: caseDef.name,
      price: caseDef.price,
      description: caseDef.description,
      image: getCaseImage(caseDef.id),
      imageEmoji: caseDef.emoji,
      theme: caseDef.theme,
      items: items,
      totalValue: totalValue.toFixed(2)
    };
  });

  return allCases;
}

